


<p id="notice"><%= notice %></p>

<div class="container">
  <div class="row">
    <H1><%= @procedure.name %> - <%= @procedure.price %></H1>
  </div>
  <div class="row">
    <% if @procedure.picture.attached? %>
      <div class="col-4">
        <image style="max-width:400px;width:100%" src="<%=(url_for(@procedure.picture)) %>" >
      </div>
    <% end %>
    <div class="col">
      <%= @procedure.description.html_safe %>
    </div>
  </div>
</div>
<br><br>
