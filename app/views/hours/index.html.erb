<p id="notice"><%= notice %></p>

<h1>Hours</h1>

<table>
  <thead>
    <tr>
      <th>Layout</th>
      <th>Dental</th>
      <th>Medical</th>
      <th>Vision</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @hours.each do |hour| %>
      <tr>
        <td><%= hour.layout %></td>
        <td><%= hour.dental %></td>
        <td><%= hour.medical %></td>
        <td><%= hour.vision %></td>
        <td><%= link_to 'Show', hour %></td>
        <td><%= link_to 'Edit', edit_hour_path(hour) %></td>
        <td><%= link_to 'Destroy', hour, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Hour', new_hour_path %>
